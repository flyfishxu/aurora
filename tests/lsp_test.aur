fn add(x: int, y: int) -> int {
    return x + y
}

fn multiply(a: double, b: double) -> double {
    return a * b
}

class Counter {
    pub let value: int
    
    pub fn increment() {
        this.value = this.value + 1
    }
    
    pub fn getValue() -> int {
        return this.value
    }
}

