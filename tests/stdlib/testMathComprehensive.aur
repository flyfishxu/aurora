// Comprehensive test for stdlib math functions
// Note: stdlib is auto-imported, no need for manual import
// Now using clean, overloaded function names

fn main() -> int {
    printd(1000)  // Start marker
    
    // Test abs functions (overloaded)
    printd(abs(5))
    printd(abs(-10))
    printd(abs(3.5))
    printd(abs(-7.2))
    
    // Test min/max (overloaded)
    printd(min(10, 20))
    printd(max(10, 20))
    printd(min(5.5, 3.3))
    printd(max(5.5, 3.3))
    
    // Test power
    printd(pow(2, 3))
    printd(pow(5, 0))
    printd(pow(3, 4))
    
    // Test factorial
    printd(factorial(5))
    printd(factorial(7))
    
    // Test fibonacci
    printd(fibonacci(10))
    printd(fibonacci(15))
    
    // Test GCD and LCM
    printd(gcd(48, 18))
    printd(lcm(12, 18))
    
    // Test prime check
    printd(isPrime(2))
    printd(isPrime(17))
    printd(isPrime(4))
    printd(isPrime(29))
    
    // Test sqrt
    printd(sqrt(4.0))
    printd(sqrt(9.0))
    printd(sqrt(25.0))
    
    // Test clamp (overloaded)
    printd(clamp(15, 10, 20))
    printd(clamp(5, 10, 20))
    printd(clamp(15.5, 10.0, 20.0))
    
    // Test sign (overloaded)
    printd(sign(-42))
    printd(sign(0))
    printd(sign(42))
    
    printd(9999)  // End marker
    return 0
}

