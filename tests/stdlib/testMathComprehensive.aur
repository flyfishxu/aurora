// Comprehensive test for stdlib math functions
// Note: stdlib is auto-imported, no need for manual import

fn main() -> int {
    printd(1000)  // Start marker
    
    // Test abs functions
    printd(absInt(5))
    printd(absInt(-10))
    printd(absDouble(3.5))
    printd(absDouble(-7.2))
    
    // Test min/max
    printd(minInt(10, 20))
    printd(maxInt(10, 20))
    printd(minDouble(5.5, 3.3))
    printd(maxDouble(5.5, 3.3))
    
    // Test power
    printd(powInt(2, 3))
    printd(powInt(5, 0))
    printd(powInt(3, 4))
    
    // Test factorial
    printd(factorial(5))
    printd(factorial(7))
    
    // Test fibonacci
    printd(fibonacci(10))
    printd(fibonacci(15))
    
    // Test GCD and LCM
    printd(gcd(48, 18))
    printd(lcm(12, 18))
    
    // Test prime check
    printd(isPrime(2))
    printd(isPrime(17))
    printd(isPrime(4))
    printd(isPrime(29))
    
    // Test sqrt
    printd(sqrtApprox(4.0))
    printd(sqrtApprox(9.0))
    printd(sqrtApprox(25.0))
    
    // Test clamp
    printd(clampInt(15, 10, 20))
    printd(clampInt(5, 10, 20))
    printd(clampDouble(15.5, 10.0, 20.0))
    
    // Test sign
    printd(signInt(-42))
    printd(signInt(0))
    printd(signInt(42))
    
    printd(9999)  // End marker
    return 0
}

